	type="text/javascript"></script>
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/assets/js/bootstrap.min.js"
	type="text/javascript"></script>
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/assets/js/scripts.js"></script>

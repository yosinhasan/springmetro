	type="text/javascript"></script>
<!-- Mobile Menu - Slicknav -->
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/assets/js/jquery.slicknav.js"
	type="text/javascript"></script>
<!-- Animate on Scroll-->
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/assets/js/jquery.visible.js"
	charset="utf-8"></script>
<!-- Slimbox2-->
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/assets/js/slimbox2.js"
	charset="utf-8"></script>
<!-- Modernizr -->
